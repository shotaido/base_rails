Upload SC problems

<br><br>
<%= form_tag import_settings_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import" %>
<% end %>

<br><br>
List of problems
<br>

<style>
table, th, td {
  border: 1px solid black;
}
</style>

<table>
    <tr>
        <th>id</th>
        <th>source</th>
        <th>edition</th>
        <th>question_type</th>
        <th>difficulty</th>
        <th>question_no</th>
        <th>original_text</th>
        <th>answer</th>
        <th>action</th>
        <th>Google</th>
    </tr>
    <%# Problemsc.where.not({:difficulty => "Low"}).order(id: :ASC).each do |sc| %>
    <% Problemsc.all.order(id: :ASC).each do |sc| %>
    <tr>
        <td><%= sc.id %></td>
        <td><%= sc.source %></td>
        <td><%= sc.edition %></td>
        <td><%= sc.question_type %></td>
        <td><%= sc.difficulty %></td>
        <td><%= sc.question_no %></td>
        <td><%= truncate(sc.original_text, :length => 100, :separator => ' ') %></td>
        <td><%= sc.answer %></td>
        <td>Delete</td>
        <td><a href ="https://www.google.com/search?q=<%= sc.original_text %>" target=”_blank”>search</a></td>
    </tr>
    <% end %>
</table>

<br>
<button type="button" onClick="window.location.href='/delete_sc'">Delete all questions</button>